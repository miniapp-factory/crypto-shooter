+      player.y = canvas.height / 2 - 40;
+      gameState = "playing";
+    }
+
+    function spawnEnemy() {
+      if (gameState !== "playing") return;
+      const type = ENEMY_TYPES[Math.floor(Math.random() * ENEMY_TYPES.length)];
+      const img = new Image();
+      img.src = type.src;
+      img.onload = () => {
+        const side = Math.floor(Math.random() * 4);
+        let x, y;
